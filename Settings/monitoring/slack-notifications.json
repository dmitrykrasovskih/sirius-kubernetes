  {
    "LogsConnectionString": "${Sirius-Slack-Noifications}",
    "SharedStorageConnString": "${Sirius-Slack-Noifications}",
    "ForwardMonitorMessagesQueueConnString": "${Sirius-Slack-Noifications}",
    "FullMessagesConnString": "${Sirius-Slack-Noifications}",
    "MutedMessagePrefixes": 
    {
    },
    "OpsgenieHost": "https://api.eu.opsgenie.com/v2/alerts",
    "OpsgenieApiKey": "${Sirius-Opsgenie-Key}"
  }
}
