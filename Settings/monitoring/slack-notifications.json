{
  "SlackIntegration": 
  {
    "Channels": 
    [
      {
        "Type": "Errors",
        "WebHookUrl": "${Sirius-Slack-Key}",
        "Opsgenie": true
      },
      {
        "Type": "Monitor",
        "WebHookUrl": "${Sirius-Slack-Key}",
        "Opsgenie": true
      }
    ],
    "Env": ""
  },
  "SlackNotifications": 
  {
    "AzureQueue": 
    {
      "ConnectionString": "${Sirius-Slack-Noifications}",
